FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    disk
    {
        type            zeroGradient;
    }

    bottomwall
    {
        type            zeroGradient;
    }

    topwall
    {
        type            fixedValue;
        value           uniform 0;
    }

    rightwall
    {
        type            zeroGradient;
    }

    leftwall
    {
        type            zeroGradient;
    }

    frontwall
    {
        type            zeroGradient;
    }

    backwall
    {
        type            zeroGradient;
    }
}

